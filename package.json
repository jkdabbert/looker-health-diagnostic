{
  "name": "looker-health-diagnostic-enhanced",
  "version": "2.0.0",
  "description": "AI-powered Looker health evaluation system with MCP integration, pagination, and query analysis",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "node test/test-diagnostic.js",
    "test-mcp": "node scripts/test-mcp.js",
    "test-ai": "node test/test-ai-analysis.js",
    "setup": "npm install && node scripts/setup.js",
    "setup-enhanced": "npm install && node scripts/setup-enhanced.js",
    "dashboard": "concurrently \"npm run dev\" \"npm run serve-dashboard\"",
    "serve-dashboard": "npx http-server public -p 3001 -o",
    "build": "echo 'Building enhanced dashboard...' && cp src/dashboard.html public/dashboard.html",
    "lint": "eslint src/ --ext .js",
    "lint:fix": "eslint src/ --ext .js --fix",
    "test:unit": "jest",
    "test:integration": "node test/integration-tests.js",
    "test:mcp-pagination": "node test/test-mcp-pagination.js",
    "validate-env": "node scripts/validate-env.js",
    "docker:build": "docker build -t looker-health-diagnostic-enhanced .",
    "docker:run": "docker run -p 3000:3000 --env-file .env looker-health-diagnostic-enhanced",
    "demo": "USE_MOCK_DATA=true npm run dev",
    "demo:ai": "USE_MOCK_DATA=true ENABLE_AI_RECOMMENDATIONS=true npm run dev",
    "benchmark": "node test/benchmark-performance.js",
    "docs": "jsdoc src/ -d docs/",
    "health-check": "curl -f http://localhost:3000/api/health || exit 1"
  },
  "keywords": [
    "looker",
    "mcp",
    "health",
    "diagnostic",
    "ai",
    "gemini",
    "business-intelligence",
    "hackathon",
    "pagination",
    "query-analysis",
    "lookml",
    "performance"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "dependencies": {
    "@looker/sdk": "^23.16.0",
    "@looker/sdk-node": "^23.16.0",
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.2.0",
    "joi": "^17.11.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "uuid": "^9.0.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "eslint": "^8.57.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.4",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/looker-health-diagnostic-enhanced.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/looker-health-diagnostic-enhanced/issues"
  },
  "homepage": "https://github.com/your-username/looker-health-diagnostic-enhanced#readme",
  "config": {
    "nodeVersion": "18.x",
    "npmVersion": "8.x"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/test/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/app.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  },
  "eslintConfig": {
    "extends": [
      "standard"
    ],
    "env": {
      "node": true,
      "jest": true
    },
    "rules": {
      "no-console": "off",
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_"
        }
      ]
    }
  },
  "nodemonConfig": {
    "watch": [
      "src/"
    ],
    "ext": "js,json",
    "ignore": [
      "node_modules/",
      "test/",
      "public/"
    ],
    "env": {
      "NODE_ENV": "development"
    }
  }
}
